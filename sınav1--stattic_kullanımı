static void Main(string[] args)
        {
            Console.WriteLine(deneme.topla(1,2,3,4,5,45,74,45));
            Console.ReadLine();
        }
        
        
        
 class deneme
    {
       public static int topla(params int[] dizi)
        {
            int toplam = 0;
            foreach (var i in dizi)
            {
                toplam = toplam + i;
            }
            return toplam;
        }
    }
